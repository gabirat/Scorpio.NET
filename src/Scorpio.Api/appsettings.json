{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:8081"
      }
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  },
  "socketClient": {
    "host": "127.0.0.1",
    "port": 5000
  },
  "Ubiquiti": {
    "EnablePoller": false,
    "SnmpAgentIp": "10.0.10.255", // 10.0.24.10    10.0.10.255
    "SnmpCommunity": "scorpio",
    "RootOid": "1.3.6.1.4.1.41112.1",
    "Oids": [
      {
        "Oid": "1.3.6.1.4.1.41112.1.4.1.1.6.1",
        "PhysicalProperty": {
          "Magnitude": "signalPower",
          "Unit": "dBm"
        }
      },
      {
        "Oid": "1.3.6.1.4.1.41112.1.4.5.1.5.1",
        "PhysicalProperty": {
          "Magnitude": "signalPower2",
          "Unit": "dBm"
        }
      },
      {
        "Oid": "1.3.6.1.4.1.41112.1.4.7.1.3.1",
        "PhysicalProperty": {
          "Magnitude": "signalPower3",
          "Unit": "dBm"
        }
      },
      {
        "Oid": "1.3.6.1.4.1.41112.1.4.1.1.4.1",
        "PhysicalProperty": {
          "Magnitude": "frequency",
          "Unit": "MHz"
        }
      }
    ]
  },
  "RabbitMq": {
    "host": "10.0.0.1",
    "port": 5672,
    "userName": "admin",
    "password": "admin",
    "virtualHost": "/",
    "exchangeName": "scorpio.direct",
    "myQueueName": "queue.rov.webapi",
    "messageTTL": "2500"
  },
  "MongoDb": {
    "ConnectionString": "mongodb://mongodbuser:mongodbpassword@127.0.0.1:27017",
    "Database": "scorpiodb"
  }
}
